{
    "Header": {
        "prefix":"header",
        "body":[
            "/*\t$TM_FILENAME",
            "",
            "\t${2:author}",
            "\t${3:company}",
            "\t$CURRENT_MONTH_NAME $CURRENT_DATE, $CURRENT_YEAR",
            "",
            "\t${5:description}",
            "*/"
        ],
        "description": "File header template"
    },
    "Region": {
        "prefix":"region",
        "body":[
            ";region ${1:region name}",
            "$2",
            ";endregion"
        ],
        "description": "Region folding"
    },
    "Defun": {
        "prefix":"defun",
        "body":[
            "(defun ${1:functionName} ($2)",
            "\t(let ()",
            "\t\t$3",
            "\t)",
            ")"
        ],
        "description": "Defines function"
    },
    "Procedure": {
        "prefix":"procedure",
        "body":[
            "(procedure ${1:functionName}($2)",
            "\t(let ()",
            "\t\t$3",
            "\t)",
            ")"
        ],
        "description": "Defines function"
    },
    "Let": {
        "prefix":"let",
        "body":[
            "(let (${1:variableName})",
            "\t$2",
            ")"
        ],
        "description": "Define local variables"
    },
    "When": {
        "prefix":"when",
        "body":[
            "(when ${1:condition}",
            "\t$2",
            ")"
        ],
        "description": "Pass if condition is non-nil"
    },
    "Unless": {
        "prefix":"unless",
        "body":[
            "(unless ${1:condition}",
            "\t$2",
            ")"
        ],
        "description": "Pass if condition is nil"
    },
    "Case": {
        "prefix":"case",
        "body":[
            "(case ${1:key}",
            "\t(${2:value}",
            "\t\t$3",
            "\t)",
            "\t(t",
            "\t\tt",
            "\t)",
            ")"
        ],
        "description": "Branches to the value equal to key"
    },
    "If": {
        "prefix":"if",
        "body":[
            "(if ${1:condition} then",
            "\t$2",
            ")"
        ],
        "description": "If"
    },
    "If Else": {
        "prefix": ["ifelse", "else"],
        "body":[
            "(if ${1:condition} then",
            "\t$2",
            "else",
            "\t$3",
            ")"
        ],
        "description": "If Else"
    },
    "Foreach": {
        "prefix":"foreach",
        "body":[
            "(foreach ${1:item} ${2:list}",
            "\t$3",
            ")"
        ],
        "description": "Foreach"
    },
    "For": {
        "prefix":"for",
        "body":[
            "(for i ${1:start} ${2:end}",
            "\t$3",
            ")"
        ],
        "description": "For"
    },
    "Setof": {
        "prefix":"setof, linq",
        "body":[
            "setof(x ${1:list} ${2:condition})"
        ],
        "description": "Filter a list"
    },
    "Mapcar Lambda": {
        "prefix": ["mapcarlambda", "lambda", "linq"],
        "body":[
            "mapcar(lambda((x) ${1:condition}) ${2:list})"
        ],
        "description": "Modify all items in a list"
    },
    "Index": {
        "prefix":"index",
        "body":[
            "index(${1:stringToEvaluate} ${2:stringToSearch})"
        ],
        "description": "Get the remainder of [stringToEvaluate]"
    },
    "Author": {
        "prefix":"author",
        "body":[
            "Herbert Agosto"
        ],
        "description": "Author"
    }
    
}
